{
	"Name":"Javascript",
	"FileTypes":[".js"],
	"CommentPrefix":"//",

	"Rules":[
		{
			"Style":"Comment",
			"Options":"Multiline",
			"Regex":"//.*$"
		},
		{
			"Style":"Comment",
			"Options":"Singleline",
			"Regex":"(/\\*.*?\\*/)|(/\\*.*)"
		},
		{
			"Style":"Comment",
			"Options":"Singleline,RightToLeft",
			"Regex":"(/\\*.*?\\*/)|(.*\\*/)"
		},
		{
			"Style":"String",
			"Regex":"\"\"|@\"\"|''|@\".*?\"|(?<!@)(?<range>\".*?[^\\\\]\")|'.*?[^\\\\]'"
		},
		{
			"Style":"ClassName",
			"Regex":"\\b(class|struct|enum|interface)\\s+(?<range>\\w+?)\\b"
		},
		{
			"Style":"FunctionName",
			"Regex":"\\b(void|int|bool|string|uint|ushort|ulong|byte)\\s+(?<range>\\w+?)\\b"
		},
		{
			"Style":"Keyword",
			"Regex":"\\b(break|case|catch|const|continue|default|delete|do|escape|unescape|else|export|for|if|in|instanceof|new|return|switch|throw|try|void|while|with|typeof)\\b",
		},
		{
			"Style":"Constant",
			"Regex":"\\b(true|false|null|this)\\b"
		},
		{
			"Style":"Storage",
			"Regex":"\\b(var|function)\\b"
		},
		{
			"Style":"LibraryClass",
			"Regex":"\\b(Array|Boolean|Date|Math|Object|String|document|NaN|prototype|window|navigator|Audio|Sound)\\b"
		},
		{
			"Style":"LibraryFunction",
			"Regex":"\\b(isFinite|isNaN|parseInt|parseFloat|length|documentElement|getElementsByTagName|getElementsByID|decodeURI|decodeURIComponent|encodeURI|eval|addEventListener|alert|atob|blur|btoa|clearInterval|clearTimeout|close|confirm|dispatchEvent|doNotTrack|find|focus|getComputedStyle|getMatchedCSSRules|getSelection|matchMedia|moveBy|moveTo|open|openDatabase|print|prompt|removeEventListener|resizeBy|resizeTo|scroll|scrollBy|scrollTo|setInterval|setTimeout|showModalDialog|stop)\\b"
		},
		{
			"Style":"Number",
			"Regex":"\\b\\d+[\\.]?\\d*([eE]\\-?\\d+)?[lLdDfF]?\\b|\\b0x[a-fA-F\\d]+\\b"
		}
	],
	
	"FoldingRules":[
		{
			"FoldingStartMarker":"{",
			"FoldingEndMarker":"}"
		},
		{
			"FoldingStartMarker":"/\\*",
			"FoldingEndMarker":"\\*/",
		}
	]
}